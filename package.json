{
  "name": "gedikuniui",
  "version": "1.0.0",
  "description": "An Interface for Gedik Univeristy's ROV",
  "main": "main.js",
  "scripts": {
    "start": "concurrently \"electron .\" \"npx tailwindcss -i ./Project/Styling/index.css -o ./public/index.css --watch\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/electron/GedikUniROVUI",
  "author": "GitHub",
  "license": "CC0-1.0",
  "files": [
    "./Project/Pictures/*"
  ],
  "dependencies": {
    "electron": "^19.0.0",
    "electron-reloader": "^1.2.3"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "electron-builder": "^23.0.3",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "tailwindcss": "^3.0.24"
  },
  "build": {
    "appId": "com.gedikuniui.app",
    "productName": "Gedik Uni ROV Interface",
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "./Project/Pictures/Icons/logo.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "./Project/Pictures/Icons/logo.ico"
    },
    "linux": {
      "target": "deb",
      "category": "Development",
      "icon": "./Project/Pictures/Icons/logo.png"
    }
  }
}
